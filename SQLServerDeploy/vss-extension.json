{
  "manifestVersion": 1,
  "id": "SQLServerDeploy",
  "name": "SQLServerDeploy",
  "version": "0.3.10",
  "publisher": "gustavoamerico",
  "public": true,
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Task for deployment dacpac file.",
  "categories": [
    "Build and release"
  ],
  "branding": {
    "color": "rgb(36, 43, 50)",
    "theme": "dark"
  },
  "icons": {
    "default": "images/icon.png",
    "large": "images/icon.png"
  },
  "scopes": [
    "vso.build_execute"
  ],
  "files": [
    {
      "path": "readme-pt-br.md"
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib/vss.sdk.min.js",
      "addressable": true
    }
  ],
  "screenshots": [
    {
      "path": "images/screenshot_1.png"
    }
  ],
  "tags": [
    "msdeploy",
    "webdeploy",
    "sqldeploy",
    "sqlserver"
  ],
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "links": {
    "learn": {
      "uri": "http://gustavoamerico.net"
    },
    "support": {
      "uri": "mailto:contato@gustavoamerico.net"
    }
  },
  "contributions": [
    {
      "id": "gustavoamerico",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "scripts"
      }
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/GustavoAmerico/SQLServerDeploy"
  }
}